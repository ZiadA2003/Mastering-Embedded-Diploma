#include "stdio.h"

int main (){
	float x,y,z;
	printf("Enter three numbers: ");
	fflush(stdout);
	scanf("%f",&x);
	scanf("%f",&y);
	scanf("%f",&z);
	if(x>y){
		if(x>z){
			printf("Largest number = %g", x);
		}
		else printf("Largest number = %g", z);
	}
	else if (y > z){
			printf("Largest number = %g", y);
	}
	else printf("Largest number = %g", z);
	return 0;
}
