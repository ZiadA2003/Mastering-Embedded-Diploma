#include "stdio.h"
#include "string.h"

int main(){
	char text[50], Rev[50];
	printf("Enter the string : ");
	fflush(stdout);
	gets(text);
	fflush(stdin);
	int len = strlen(text), j = 0;

	for(int i = len-1; i >= 0; i--){
		Rev[i] = text[j];
		j++;
	}

	printf("Reverse string is : ");
	return 0;

}
