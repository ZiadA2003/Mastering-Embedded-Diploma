#include "stdio.h"

int main(){
	float a[2][2], b[2][2], sum[2][2];
	printf("Enter the elements of 1st matrix");

	for(int i = 0; i < 2; i++){
		for(int j = 0; j < 2; j++){
			printf("Enter a%d%d\n: ");
			fflush(stdin); fflush(stdout);
			scanf("%f", a[i][j]);
		}
	}

	for(int i = 0; i < 2; i++){
			for(int j = 0; j < 2; j++){
				printf("Enter b%d%d\n: ");
			}
		}
	return 0;
}
