 */

#include "stdio.h"

int main(){
	int r,c;
	int matrix[20][20];

	for(int i = 0; i<r; i++){
	       for(int j = 0; j < c; j++){
	           printf("Enter element a%d%d: ", i+1, j+1);
	           scanf("%d", matrix[i][j]);
	       }
	   }

	   for(int i = 0; i<c; i++){
	       for(int j = 0; j < r; j++){
	           printf("%d\t", matrix[j][i]);
	       }
	       printf("\n");
	   }
	}

	return 0;
}
