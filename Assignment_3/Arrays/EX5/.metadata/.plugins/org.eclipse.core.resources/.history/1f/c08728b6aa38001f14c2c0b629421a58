#include "stdio.h"

int main(){

	int arr [100];
	int n, element;
	printf("Enter no of elements : ");
	fflush(stdout);
	scanf("%d", &n);

	for(int i = 0; i< n; i++){
		arr[i] = (i+1)*11;
		printf("%d ", arr[i]);
	}

	printf("\nEnter the element to be searched : ");
	fflush(stdout);
	scanf("%d", &element);


	for(int i = 0; i < n; i++){
		printf("%d ", arr[i]);
	}


	return 0;
}

